// prints out given range
// author


#include <stdio.h>

int main(void) {
    int start;
    int finish;
    
    printf("Enter start: ");
    scanf("%d", &start);
        
    printf("Enter finish: ");
    scanf("%d", &finish);

    while (start <= finish) {
        printf("%d\n", start);
        start++;
        // start += 1
        // start = start + 1
    }


    return 0;
}


